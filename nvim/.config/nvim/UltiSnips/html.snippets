snippet '!' "HTML Skeleton" br
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>${1:Document}</title>
</head>
<body>
    $2
</body>
</html>
endsnippet

snippet scriptsrc "Script element with empty src attribute" i
<script src="$1">$0</script>
endsnippet
